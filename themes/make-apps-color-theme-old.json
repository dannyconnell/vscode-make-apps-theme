{
  "$schema": "vscode://schemas/color-theme",
  "name": "Make Apps with Danny Theme",
  "type": "dark",
  "colors": {
    "panel.background": "#07252c",
    "panelTitle.inactiveForeground": "#a1d7da99",
    "panelTitle.activeForeground": "#a1d7da",
    "panelTitle.activeBorder": "#a1d7da",
    "focusBorder": "#00000000",
    "contrastBorder": "#07252c",
    "notifications.background": "#00abc199",
    "activityBar.background": "#07252c",
    "activityBar.foreground": "#a1d7da",
    "activityBar.inactiveForeground": "#a1d7da99",
    "activityBarBadge.background": "#404954",
    "activityBarBadge.foreground": "#ffffff",
    "input.background": "#002b36",
    "inputValidation.infoBackground": "#002b36",
    "input.border": "#ffffff33",
    "inputValidation.infoBorder": "#19f9d8",
    "inputOption.activeBorder": "#19f9d8",
    "inputValidation.errorBorder": "#19f9d8",
    "editor.background": "#092C34",
    "editorCursor.foreground": "#F8F8F0",
    "editor.foreground": "#E6E6E6",
    "editorWhitespace.foreground": "#34383D",
    "editor.lineHighlightBackground": "#062127",
    "editorLineNumber.foreground": "#ffffff11",
    "editorLineNumber.activeForeground": "#ffffff",
    "editor.selectionBackground": "#00abc199",
    "editor.selectionForeground": "#fff",
    "editorError.foreground": "#ff590077",
    "editorError.border": "#092C34",
    "editorIndentGuide.background": "#ffffff11",
    "editorIndentGuide.activeBackground": "#ffffff33",
    "editorOverviewRuler.border": "#00000000",
    "list.hoverBackground": "#00abc122",
    "list.hoverForeground": "#d4d4d4",
    "list.focusBackground": "#09698180",
    "list.focusForeground": "#d4d4d4",
    "list.activeSelectionBackground": "#00abc122",
    "list.activeSelectionForeground": "#d4d4d4",
    "list.inactiveSelectionBackground": "#00abc122",
    "list.inactiveSelectionForeground": "#aeb1b0",
    "list.highlightForeground": "#aeb1b0",
    "list.inactiveFocusBackground": "#002b36",
    "sideBar.background": "#07252c",
    "sideBar.foreground": "#a1d7da99",
    "sideBar.border": "#07252c",
    "sideBarTitle.foreground": "#a1d7da",
    "sideBarSectionHeader.foreground": "#a1d7da99",
    "gitDecoration.deletedResourceForeground": "#ff590099",
    "gitDecoration.conflictingResourceForeground": "#F1C23299",
    "gitDecoration.modifiedResourceForeground": "#00abc199",
    "gitDecoration.untrackedResourceForeground": "#EF8A1799",
    "gitDecoration.ignoredResourceForeground": "#a1d7da33",
    "tree.indentGuidesStroke": "#ffffff11",
    "titleBar.activeBackground": "#002b36",
    "titleBar.activeForeground": "#aeb1b0",
    "titleBar.inactiveBackground": "#002b36",
    "titleBar.inactiveForeground": "#aeb1b0",
    "tab.activeBackground": "#07252c",
    "tab.activeForeground": "#a1d7da",
    "tab.activeBorder": "#00abc1",
    "tab.inactiveBackground": "#07252c",
    "tab.inactiveForeground": "#a1d7da99",
    "breadcrumb.background": "#07252c",
    "breadcrumb.foreground": "#ffffff44",
    "breadcrumb.focusForeground": "#ffffff77",
    "extensionButton.prominentForeground": "#404954",
    "extensionButton.prominentBackground": "#00abc1",
    "extensionButton.prominentHoverBackground": "#19f9d7c7",
    "editorWidget.background": "#002b36",
    "statusBar.background": "#062127",
    "statusBar.foreground": "#aeb1b0",
    "statusBar.debuggingBackground": "#002b36",
    "statusBar.noFolderBackground": "#002b36",
    "statusBarItem.activeBackground": "#002b36",
    "statusBarItem.prominentBackground": "#002b36",
    "statusBarItem.prominentHoverBackground": "#09698180",
    "button.background": "#00abc1",
    "button.foreground": "#002b36",
    "button.hoverBackground": "#19f9d7c7",
    "dropdown.background": "#092C34",
    "dropdown.listBackground": "#002b36",
    "dropdown.border": "#ffffff10",
    "debugToolBar.background": "#002b36",
    "editorGroupHeader.tabsBackground": "#07252c",
    "textLink.activeForeground": "#b3b8b6e5",
    "textLink.foreground": "#00abc1",
    "textSeparator.foreground": "#ff0000",
    "scrollbarSlider.background": "#ffffff11",
    "scrollbarSlider.hoverBackground": "#ffffff22",
    "scrollbarSlider.activeBackground": "#ffffff33"
  },
  "tokenColors": [
    {
      "name": "comentarios",
      "scope": "comment",
      "settings": {
        "foreground": "#8a8787",
        "fontStyle": "italic"
      }
    },
    {
      "name": "const, let, import",
      "scope": "storage.type, keyword.control.import.js, keyword.control.from.js, support.class.error.js",
      "settings": {
        "foreground": "#ff5900"
      }
    },
    {
      "name": "string between quotes and template literals",
      "scope": "string.quoted.double, string.template",
      "settings": {
        "foreground": "#ffffffdd"
      }
    },
    {
      "name": "color quotes double, simple and template",
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end, punctuation.definition.string.template.begin, punctuation.definition.string.template.end",
      "settings": {
        "foreground": "#00acc1"
      }
    },
    {
      "scope": "meta.brace.round.js, punctuation.terminator.statement.js",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "name": "{} , ()",
      "scope": "punctuation.separator.comma.js, punctuation.definition.block.js, punctuation.definition.parameters.begin.js, punctuation.definition.parameters.end.js",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "scope": "keyword.control.loop",
      "settings": {
        "foreground": "#09cbdd"
      }
    },
    {
      "scope": "keyword.control.flow, keyword.operator.new",
      "settings": {
        "foreground": "#00e5ffcc"
      }
    },
    {
      "name": "template expression ${} and comparation",
      "scope": "punctuation.accessor, meta.brace.square.js, keyword.operator.arithmetic, keyword.operator.comparison, keyword.operator.assignment, keyword.operator.relational, keyword.operator, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end, constant.language.import-export-all.js, entity.other.attribute-name.js",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "scope": "keyword.control.switch",
      "settings": {
        "foreground": "#079ba8"
      }
    },
    {
      "name": "sigle string quote",
      "scope": "string.quoted.single",
      "settings": {
        "foreground": "#00abc1"
      }
    },
    {
      "name": "this",
      "scope": "variable.language.this, constant.language.boolean.true, constant.language.boolean.false, constant.language.undefined, constant.language.null",
      "settings": {
        "foreground": "#ff5900"
      }
    },
    {
      "name": "variable object",
      "scope": "variable.other.object.property, variable.other.property, meta.template.expression, support.variable.property, variable.language.super, support.class.component.js, entity.name.tag.js",
      "settings": {
        "foreground": "#EF8A17"
      }
    },
    {
      "scope": "meta.jsx.children.js",
      "settings": {
        "foreground": "#c0babaec"
      }
    },
    {
      "name": "keys",
      "scope": "meta.embedded.line, variable.other.object, variable.other.readwrite, entity.name.type.class, entity.name.type,  entity.other.inherited-class, variable.parameter, variable.object.property.js",
      "settings": {
        "foreground": "#a1d7da"
      }
    },
    {
      "name": "number",
      "scope": "constant.numeric.decimal",
      "settings": {
        "foreground": "#ff5622de"
      }
    },
    {
      "name": "console.log",
      "scope": "support.class.console, support.function.console, meta.object-literal.key",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "name": "function and if/else",
      "scope": " support.variable.dom, storage.modifier, support.function",
      "settings": {
        "foreground": "#079ba8"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#09cbdd"
      }
    },
    {
      "scope": " storage.type.function",
      "settings": {
        "foreground": "#f48fb1"
      }
    },
    {
      "name": "json config",
      "scope": "support.type.property-name.json, punctuation.support.type.property-name.begin.json, punctuation.support.type.property-name.end.json",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "name": "if else, try catch",
      "scope": "keyword.control.conditional, keyword.control.trycatch.js",
      "settings": {
        "foreground": "#09cbdd"
      }
    },
    {
      "scope": "support.function.dom",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#00acc1"
      }
    },
    {
      "scope": "constant.language.json, constant.numeric.json",
      "settings": {
        "foreground": "#ee475e"
      }
    },
    {
      "name": "html tag",
      "scope": "entity.name.tag.script.html, entity.name.tag.structure.any.html, entity.name.tag.html, entity.name.tag.inline.any.html, entity.name.tag.block.any.html, entity.name.tag.other.html, meta.tag.sgml.doctype.html, entity.name.tag.style.html",
      "settings": {
        "foreground": "#EF8A17"
      }
    },
    {
      "name": "src & alt ...",
      "scope": "entity.other.attribute-name.html, entity.other.attribute-name.id.html",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "name": "gt and lt",
      "scope": "punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html, punctuation.definition.tag.begin.js, punctuation.definition.tag.end.js",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "scope": "meta.tag.structure.any.html, meta.tag.inline.any.html",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "name": "text html",
      "scope": "text.html.basic",
      "settings": {
        "foreground": "#bdbdbd"
      }
    },
    {
      "scope": "meta.toc-list.id.html",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    // xml
    {
      "scope": "entity.name.tag.xml, entity.name.tag.localname.xml, entity.name.tag.namespace.xml",
      "settings": {
        "foreground": "#ff2c6bc2"
      }
    },
    {
      "scope": "punctuation.definition.tag.xml, meta.tag.preprocessor.xml, punctuation.separator.namespace.xml",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "scope": "string.quoted.double.xml",
      "settings": {
        "foreground": "#728e9e"
      }
    },
    {
      "scope": "entity.other.attribute-name.xml, entity.other.attribute-name.localname.xml",
      "settings": {
        "foreground": "#33fae9ad"
      }
    },
    {
      "scope": "text.xml",
      "settings": {
        "foreground": "#8bdbe9e3"
      }
    },
    // yanl
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#e4e0bd"
      }
    },
    {
      "scope": "source.yaml",
      "settings": {
        "foreground": "#00ffffcc"
      }
    },
    // editor config
    {
      "name": "editor config",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "name": "editor config",
      "scope": "source.ini",
      "settings": {
        "foreground": "#00abc1"
      }
    },
    // css
    {
      "scope": "entity.other.attribute-name.class.css, entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#F1C232"
        // "foreground": "#34eeeed5"
      }
    },
    {
      "scope": "support.type.property-name.css, support.constant.color.w3c-extended-color-name.css, support.constant.color.w3c-standard-color-name.css",
      "settings": {
        "foreground": "#a0b9c4c0"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#ff2c6be0"
      }
    },
    {
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "scope": "keyword.other.unit, keyword.control.at-rule.include.scss, meta.definition.variable.scss, keyword.control.at-rule.import.scss, keyword.control.return.scss, entity.name.tag.wildcard.css, entity.name.tag.wildcard.scss, keyword.control.at-rule.mixin.scss",
      "settings": {
        "foreground": "#ff2c6bc2"
      }
    },
    {
      "scope": "punctuation.section.property-list.begin.bracket.curly.scss, punctuation.section.property-list.end.bracket.curly.scss, punctuation.section.property-list.begin.bracket.curly.css, punctuation.section.property-list.end.bracket.curly.css, punctuation.separator.list.comma.css, punctuation.section.function.begin.bracket.round.css, punctuation.section.function.end.bracket.round.css, punctuation.definition.entity.begin.bracket.square.css, punctuation.definition.entity.end.bracket.square.css",
      "settings": {
        "foreground": "#b9b8b8"
      }
    },
    {
      "scope": "punctuation.terminator.rule.css, punctuation.separator.key-value.css",
      "settings": {
        "foreground": "#a7a2a2"
      }
    },
    {
      "scope": "punctuation.definition.constant.css",
      "settings": {
        "foreground": "#a0b9c4c0"
      }
    },
    {
      "scope": "constant.other.color.rgb-value.hex.css, entity.name.tag.reference.scss",
      "settings": {
        "foreground": "#dd8aa3"
      }
    },
    {
      "scope": "support.type.vendored.property-name.css",
      "settings": {
        "foreground": "#a7a2a2"
      }
    },
    {
      "scope": "keyword.control.at-rule.keyframes.css, entity.other.attribute-name.placeholder.css, keyword.control.at-rule.media.css, support.type.property-name.media.css, support.constant.media.css, support.constant.font-name.css, keyword.other.important.scss, keyword.control.at-rule.media.scss, keyword.control.operator.css.scss, entity.other.keyframe-offset.css, entity.other.attribute-name.css, keyword.control.at-rule.import.css, keyword.control.for.scss, keyword.control.if.scss, meta.at-rule.include.scss",
      "settings": {
        "foreground": "#dd8aa3"
      }
    },
    {
      "scope": "variable.parameter.keyframe-list.css, meta.property-value.css",
      "settings": {
        "foreground": "#7cc0d1"
      }
    },
    {
      "scope": "support.constant.property-value.css",
      "settings": {
        "foreground": "#7cc0d1"
        // "foreground": "#34eeeeb9"
      }
    },
    {
      "scope": "entity.other.attribute-name.pseudo-element.css, entity.other.attribute-name.pseudo-class.css",
      "settings": {
        "foreground": "#a0b9c4fa"
      }
    },
    {
      "scope": "entity.other.keyframe-offset.percentage.css, meta.property-list.scss",
      "settings": {
        "foreground": "#34eeeed7"
      }
    },
    {
      "scope": "variable.scss, support.function.url.css, entity.other.attribute-name.attribute.scss, punctuation.definition.attribute-selector.begin.bracket.square.scss, punctuation.definition.attribute-selector.end.bracket.square.scss",
      "settings": {
        "foreground": "#88f1ffc2"
      }
    },

    // markdown
    {
      "name": "markdown",
      "scope": "markup.heading.markdown, punctuation.definition.metadata.markdown, punctuation.definition.list.begin.markdown, meta.separator.markdown, punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#93A8AC"
      }
    },
    {
      "scope": "beginning.punctuation.definition.list.markdown",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    // Vue configs
    {
      "scope": "entity.name.tag.template.html, keyword.control.export.js",
      "settings": {
        "foreground": "#EF8A17"
      }
    },
    {
      "scope": "text.html.vue-html",
      "settings": {
        "foreground": "#bdbdbd"
      }
    },
    {
      "scope": "keyword.control.default.js, support.class.builtin.js, meta.function-call.js, keyword.control.as.js",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    // webpack
    {
      "scope": "support.type.object.module.js, string.regexp.js, support.variable.object.node.js",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    {
      "scope": "support.variable.object.process.js, variable.other.constant.property.js, variable.other.constant.js",
      "settings": {
        "foreground": "#a1d7da"
      }
    },
    // Php
    {
      "scope": "punctuation.definition.variable.php",
      "settings": {
        "foreground": "#F1C232"
      }
    },
    {
      "scope": "variable.other.php, variable.other.global.php, variable.other.global.safer.php",
      "settings": {
        "foreground": "#26e0d7f3"
      }
    },
    {
      "scope": "constant.language.php, constant.other.php",
      "settings": {
        "foreground": "#EF8A17"
      }
    },
    {
      "scope": "keyword.control.return.php",
      "settings": {
        "foreground": "#09cbdd"
      }
    },
    {
      "scope": "keyword.other.new.php, keyword.other.use.php, keyword.other.namespace.php, keyword.other.use-as.php, keyword.other.function.use.php, keyword.other.clone.php, keyword.control.yield.php, keyword.control.break.php, keyword.control.die.php",
      "settings": {
        "foreground": "#f48fb1"
      }
    },
    {
      "scope": "support.class.builtin.php, entity.name.function.php, entity.other.alias.php, support.constant.std.php, constant.other.class.php",
      "settings": {
        "foreground": "#64f5ffda"
      }
    },
    {
      "scope": "support.class.php, support.other.namespace.php, variable.other.class.php, variable.other.property.php, support.constant.core.php, support.constant.ext.php, support.class.exception.php",
      "settings": {
        "foreground": "#a0e3e7"
      }
    },
    {
      "scope": "storage.modifier.php",
      "settings": {
        "foreground": "#09cbdd"
      }
    },
    {
      "scope": "keyword.control.case.php, keyword.control.switch.php, keyword.control.default.php, keyword.control.if.php, keyword.control.foreach.php, keyword.control.continue.php, keyword.other.class.php, keyword.control.elseif.php, keyword.control.else.php, keyword.control.while.php, keyword.control.for.php, keyword.control.do.php",
      "settings": {
        "foreground": "#a7deec"
      }
    },
    {
      "scope": "punctuation.section.embedded.begin.php, punctuation.section.embedded.end.php",
      "settings": {
        "foreground": "#a7deec"
      }
    },
    {
      "scope": "keyword.control.exception.php, keyword.control.exception.catch.php, keyword.control.import.include.php",
      "settings": {
        "foreground": "#eb9485"
      }
    },
    {
      "scope": "string.regexp.single-quoted.php",
      "settings": {
        "foreground": "#eb9485"
      }
    },
    {
      "scope": "keyword.other.phpdoc.php, keyword.other.type.php",
      "settings": {
        "foreground": "#53a5b9"
      }
    }
  ]
}
